@model IEnumerable<WebApplication3.CurrencyRate>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CurrencyRateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AverageRate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndOfDayRate)
