        <td>
            @Html.DisplayFor(modelItem => item.CurrencyRateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AverageRate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndOfDayRate)
