            @Html.DisplayFor(modelItem => item.Currency.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Currency1.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CurrencyRateID }) |
